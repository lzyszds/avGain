{
  "name": "AvGain",
  "private": true,
  "version": "1.1.0",
  "main": "dist-electron/main/index.js",
  "description": "av爬取软件",
  "author": "lzyszds",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "chcp 65001 && vite ",
    "clean": "tsc --build --clean",
    "create": "node createSeprate.js",
    "viteBuild": "vite build",
    "build": "tsc && vite build && electron-builder && tsc --build --clean",
    "electronBuild": "electron-builder"
  },
  "build": {
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/logo256.png",
      "compression": "maximum",
      "artifactName": "AVGain_Setup_${version}.${ext}",
      "extraResources": [
        {
          "from": "public/logo256.png",
          "to": "build/win-unpacked/resources"
        }
      ]
    }
  },
  "dependencies": {
    "@globalhive/vuejs-tour": "^1.3.7",
    "@iconify-icons/dashicons": "^1.2.4",
    "@iconify/vue": "^4.1.1",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/color": "^3.0.3",
    "@types/node": "^20.4.6",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vueuse/core": "^10.6.1",
    "@vueuse/shared": "^10.6.1",
    "autoprefixer": "^10.4.14",
    "axios": "^1.5.0",
    "element-plus": "^2.3.8",
    "https": "^1.0.0",
    "p5": "^1.7.0",
    "plyr": "^3.7.8",
    "postcss": "^8.4.27",
    "progress": "^2.0.3",
    "qs": "^6.11.2",
    "sass": "^1.64.1",
    "side-channel": "^1.0.4",
    "superagent": "^8.1.2",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "vite-plugin-electron": "^0.12.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.3.4",
    "vue-demi": "^0.14.6",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.8"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@electron/remote": "^2.0.10",
    "electron": "^25.3.2",
    "electron-builder": "^24.9.1",
    "electron-win-state": "^1.1.22",
    "vite": "^5.0.4"
  }
}
