{
  "name": "av-gain",
  "version": "2.0.2",
  "main": "dist-electron/main/index.js",
  "description": "av爬取软件",
  "author": "lzyszds@qq.com",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "build": {
    "productName": "AvGain",
    "copyright": "Copyright © 2024 lzyszds ",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/logo256.png",
      "compression": "maximum",
      "artifactName": "AVGain_Setup_${version}.${ext}",
      "extraResources": [
        {
          "from": "public/logo256.png",
          "to": "build/win-unpacked/resources"
        }
      ]
    }
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vite",
    "clean": "tsc --build --clean",
    "create": "node createSeprate.js",
    "viteBuild": "vite build",
    "build": "vite build && electron-builder",
    "electronBuild": "electron-builder --win"
  },
  "dependencies": {
    "@globalhive/vuejs-tour": "^1.3.7",
    "@iconify-icons/dashicons": "^1.2.4",
    "@iconify/vue": "^4.1.1",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/color": "^3.0.3",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "^20.11.30",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vueuse/core": "^10.6.1",
    "@vueuse/shared": "^10.6.1",
    "autoprefixer": "^10.4.14",
    "axios": "^1.5.0",
    "cross-env": "^7.0.3",
    "echarts": "^5.5.0",
    "electron-dl": "^4.0.0",
    "electron-store": "^9.0.0",
    "element-plus": "^2.3.8",
    "fluent-ffmpeg": "^2.1.2",
    "https": "^1.0.0",
    "m3u8-parser": "^7.1.0",
    "plyr": "^3.7.8",
    "postcss": "^8.4.27",
    "progress": "^2.0.3",
    "sass": "^1.64.1",
    "sudo-prompt": "^9.2.1",
    "superagent": "^8.1.2",
    "tailwindcss": "^3.3.3",
    "thread-pool": "^0.0.1",
    "threads": "^1.7.0",
    "ts-node": "^10.9.1",
    "vue-demi": "^0.14.6",
    "vue-router": "^4.2.5",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^29.1.1",
    "electron-builder": "^24.13.3",
    "electron-win-state": "^1.1.22",
    "@electron/remote": "^2.0.10",
    "typescript": "^5.4.2",
    "vite": "^5.1.5",
    "vite-plugin-electron": "^0.28.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6",
    "unplugin-auto-import": "^0.16.6"
  }
}
